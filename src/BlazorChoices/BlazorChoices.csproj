<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<PackageId>BlazorChoices</PackageId>
		<Version>0.0.1</Version>
		<Authors>Antoine Griffard</Authors>
		<Description>Blazor component using Choices.</Description>
		<PackageTags>blazor, choices</PackageTags>
		<RepositoryUrl>https://github.com/agriffard/BlazorChoices</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/agriffard/BlazorChoices</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\README.md" Pack="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="10.0.1" />
	</ItemGroup>

</Project>
